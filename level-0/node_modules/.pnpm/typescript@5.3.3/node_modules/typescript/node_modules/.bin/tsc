#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/liuxing/Desktop/space/train/nestjs-train/nestjs-train/level-0/node_modules/.pnpm/typescript@5.3.3/node_modules/typescript/bin/node_modules:/Users/liuxing/Desktop/space/train/nestjs-train/nestjs-train/level-0/node_modules/.pnpm/typescript@5.3.3/node_modules/typescript/node_modules:/Users/liuxing/Desktop/space/train/nestjs-train/nestjs-train/level-0/node_modules/.pnpm/typescript@5.3.3/node_modules:/Users/liuxing/Desktop/space/train/nestjs-train/nestjs-train/level-0/node_modules/.pnpm/node_modules:/Users/liuxing/Desktop/space/train/nestjs-train/nestjs-train/level-0/node_modules:/Users/liuxing/Desktop/space/train/nestjs-train/nestjs-train/node_modules:/Users/liuxing/Desktop/space/train/nestjs-train/node_modules:/Users/liuxing/Desktop/space/train/node_modules:/Users/liuxing/Desktop/space/node_modules:/Users/liuxing/Desktop/node_modules:/Users/liuxing/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/liuxing/Desktop/space/train/nestjs-train/nestjs-train/level-0/node_modules/.pnpm/typescript@5.3.3/node_modules/typescript/bin/node_modules:/Users/liuxing/Desktop/space/train/nestjs-train/nestjs-train/level-0/node_modules/.pnpm/typescript@5.3.3/node_modules/typescript/node_modules:/Users/liuxing/Desktop/space/train/nestjs-train/nestjs-train/level-0/node_modules/.pnpm/typescript@5.3.3/node_modules:/Users/liuxing/Desktop/space/train/nestjs-train/nestjs-train/level-0/node_modules/.pnpm/node_modules:/Users/liuxing/Desktop/space/train/nestjs-train/nestjs-train/level-0/node_modules:/Users/liuxing/Desktop/space/train/nestjs-train/nestjs-train/node_modules:/Users/liuxing/Desktop/space/train/nestjs-train/node_modules:/Users/liuxing/Desktop/space/train/node_modules:/Users/liuxing/Desktop/space/node_modules:/Users/liuxing/Desktop/node_modules:/Users/liuxing/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsc" "$@"
else
  exec node  "$basedir/../../bin/tsc" "$@"
fi
